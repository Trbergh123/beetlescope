<!-- Begin Page Content -->
<div class="container-fluid">

  <!-- Page Heading -->
  <div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Project</h1>

  </div>

  <!-- Content Row -->
  <div class="row">

    <div class="font-weight-bold text-dark col-6">Project Name<div class="mt-3">Add Project name put</div>
    </div>
    <div class="col-6 font-weight-bold text-dark">Project description<div class="mt-3">Add project example put</div>
    </div>
    <div class="font-weight-bold col-6">
      
      Tasks
      <a class="btn-sml txt-sml btn-primary mr-2" href="Addissue.html" role="button">Add Task</a>
      <div class="container mt-3">list of tasks</div>
      {{#each tasks}}
      <ul id="new_tasks">
        {{title}}
        <li class="mt-3">{{title}}<a class="btn btn-primary ml-2" href="" role="button">remove</a></li><a
          class="btn btn-primary mr-2" href="" role="button">Details</a>
      </ul>

      {{/each}}
    </div>
  </div>

  <div class="row mt-4">

    <div class="font-weight-bold col-6">Assigned<a class="btn-sml txt-sml btn-primary ml-2" href="" role="button">Add
        User</a>
      <div class=" mt-3 container">Table of Assigend</div>
      <ul>
        {{#each users}}
        <li class="mt-3">{{username}}<a class="btn btn-primary ml-2" href="" role="button">remove</a></li>
        {{/each}}
        <li class="mt-3">Add users assigned<a class="btn btn-primary ml-2" href="" role="button">remove</a></li>
        <li class="mt-3">Add users assigned<a class="btn btn-primary ml-2" href="" role="button">remove</a></li>
      </ul>
    </div>

  </div>
</div>
<!-- End of Main Content -->






</div>
