            <!-- Begin Page Content -->
        <div  class="container-fluid">

          <!-- Page Heading -->
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="h3 mb-0 text-gray-800">Create Project</h1>
            
          </div>

          <!-- Content Row -->
          
          </div>
          
          <form id="new-project-form">
          <div class="container mt-5">
          <div class="row align-items-center justify-content-center">
            <div class="input-group mb-3">
                <div class="input-group-prepend">
                  <span class="input-group-text" id="inputGroup-sizing-default">Name of project</span>
                </div>
                <input type="text" class="form-control" aria-label="Default" aria-describedby="inputGroup-sizing-default" id="project_title">
              </div>
              <div>
                 <button type="submit" id="submit-project" onclick="pushproject()" class="btn btn-info">
                  Add
                </button>
                <div class="form-row">
                  <div class="form-group col-12">
                    <label for="exampleFormControlTextarea1">Description</label>
                    <textarea class="form-control" id="project_text" rows="3"></textarea>
                  </div>
                  <div class="form-group col-6">
                    <label for="exampleFormControlSelect1">Priority</label>
                    <select class="form-control" >
                      <option class="priority">High</option>
                      <option class="priority">Normal</option>
                      <option class="priority">Low</option>
                    </select>
                </div>
                
                <div class="form-group col-6">
                  <label for="assignee">Assignee</label>
                  <select class ="form-control" id="select-user">
                      {{#each users}}
                      <option>{{username}}</option>

                      {{/each}}

                      
                </div>
               
                   

                
              

            </div>

          </div>
        </div>
        </form>


          

          <!-- Content Row -->
          <div class="container"></div>

         
        <!-- /.container-fluid -->

      </div>
      <script src="/javascript/newproject.js"></script>
      <script src="/controllers/newproject-routes.js">
