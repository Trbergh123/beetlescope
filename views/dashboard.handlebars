<!-- Begin Page Content -->
<div class="container-fluid">

  <!-- Page Heading -->
  <div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Home Page</h1>

  </div>


  <!-- Content Row -->
  <div class="row">

    <!-- Earnings (Monthly) Card Example -->
    <div class="col-xl-3 col-md-6 mb-4">
      <div class="card border-left-primary shadow h-100 py-2">
        <div class="card-body">
          <div class="row no-gutters align-items-center">
            <div class="col mr-2">
              <div class="text-xl font-weight-bold text-primary text-uppercase mb-1">Add Project</div>



              <div class="h5 mb-0 font-weight-bold text-gray-800">Click to add a project to work on</div>
              <a class="btn btn-primary mt-5 " href="/newproject" role="button">New Project</a>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Earnings (Monthly) Card Example -->
    <div class="col-xl-3 col-md-6 mb-4">
      <div class="card border-left-success shadow h-100 py-2">
        <div class="card-body">
          <div class="row no-gutters align-items-center">
            <div class="col mr-2">
              <div class="text-xl font-weight-bold text-success text-uppercase mb-1">Add a Task</div>
              <div class="h5 mb-0 font-weight-bold text-gray-800">Click to add a task to a project</div>
              <a class="btn btn-success mt-5" href="/newtask" role="button">New task</a>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Earnings (Monthly) Card Example -->
    <div class="col-xl-3 col-md-6 mb-4">
      <div class="card border-left-info shadow h-100 py-2">
        <div class="card-body">
          <div class="row no-gutters align-items-center">
            <div class="col mr-2">
              <div class="text-xl font-weight-bold text-info text-uppercase mb-1">Add user</div>
              <div class="h5 mb-0 font-weight-bold text-gray-800">Click to add a user to project</div>
              <a class="btn btn-info mt-5" href="/edit-project" role="button">Add user</a>
            </div>
          </div>
        </div>
      </div>
    </div>



    <!-- Pending Requests Card Example -->
    <div class="col-xl-3 col-md-6 mb-4">
      <div class="card border-left-warning shadow h-100 py-2">
        <div class="card-body">
          <div class="row no-gutters align-items-center">
            <div class="col mr-2">
              <div class="text-xs font-weight-bold text-warning text-uppercase mb-1">Recent Changes</div>
              <div class="h5 mb-0 font-weight-bold text-gray-800"></div>
            </div>
            <div class="col-auto">
              <i class="fas fa-comments fa-2x text-gray-300"></i>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Content Row -->

  <div class="row">

    <!-- Area Chart -->
    <div class="col-xl-6 col-lg-7">

      <!-- Collapsable Card Example -->
      <div class="card shadow mb-4">
        <!-- Card Header - Accordion -->
        <a href="#collapseCardExample" class="d-block card-header py-3" data-toggle="collapse" role="button"
          aria-expanded="true" aria-controls="collapseCardExample">
          <h6 class="m-0 font-weight-bold text-primary">Current Project progress</h6>
        </a>
        <!-- Card Content - Collapse -->
        <div class="collapse show" id="collapseCardExample">
          <div class="card-body">
            <ul>
              {{#each projects}}
              <li class="mt-3">
                {{title}}
                <div class="progress">
                  <div id="myBar" class="progress-bar"  role="progressbar" aria-valuenow="20" aria-valuemin="0"
                    aria-valuemax="100" style="width:20%">
                    To do
                  </div>
                </div>
                <a class="btn btn-primary mt-2 text-white" onclick="move()" role="button">In progress</a>
                <a class="btn btn-primary mt-2 text-white" onclick="move2()" role="button">Completed</a>
                <script>
                  function move() {
                    var elem = document.getElementById("myBar");
                    var width = 20;
                    var id = setInterval(frame, 10);
                    var del = document.getElementById("myBar").innerHTML='';
                    var insert = document.getElementById("myBar").innerHTML='In progress';
                    function frame() {
                      if (width >= 50) {
                        clearInterval(id);
                      } else {
                        width++;
                        elem.style.width = width + '%';
                        document.getElementById("demo").innerHTML = width * 1 + '%';
                      }
                    }
                  }
                </script>
                <script>
                  function move2() {
                    var elem = document.getElementById("myBar");
                    var width = 20;
                    var id = setInterval(frame, 10);
                    var del = document.getElementById("myBar").innerHTML='';
                    var insert = document.getElementById("myBar").innerHTML='Completed';
                    function frame() {
                      if (width >= 100) {
                        clearInterval(id);
                      } else {
                        width++;
                        elem.style.width = width + '%';
                        document.getElementById("demo").innerHTML = width * 1 + '%';
                      }
                    }
                  }
                </script>
              </li>
              {{/each}}
              <script>

              </script>



            </ul>


          </div>
        </div>
      </div>
    </div>





  </div>
  <!-- /.container-fluid -->

</div>
<!-- End of Main Content -->
